{
  "name": "bavix/nova-permissions",
  "description": "Laravel Nova Grouped Permissions.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:bavix/laravel-nova.git"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "illuminate/database": "~5.7.0|~5.8.0",
    "illuminate/validation": "~5.7.0|~5.8.0",
    "spatie/laravel-permission": "^2.37",
    "laravel/nova": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Bavix\\NovaPermissions\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bavix\\NovaPermissions\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
